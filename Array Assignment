package Week1_Day2;

public class Arrays 
{


	public static void main(String[] args) 
{	
	int [] data = {11,7,6,4,3,2};
	System.out.println ("The second number from the last is "+ data[4]);
	
	int[] array = {1,2,3,4,7,6,8};
	// I have no idea what this code below githubis doing but it printed it in ascending order
	for (int i = 0; i < array.length; i++)
	{
		System.out.println(array[i]);
	}
}

}
